<%= simple_form_for(@company) do |f| %>
    <%= f.input :name, placeholder: "what's the name of the company?"  %>
    <%= f.input :description, placeholder: "tell us about your company"  %>
    <%= f.input :Logo, as: :file, placeholder: "add the logo of your company" %>
    <%= f.submit :Registrer, class: "btn btn-primary" %>
<% end %>
